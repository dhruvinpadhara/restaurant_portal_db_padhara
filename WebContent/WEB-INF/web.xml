<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Design</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>FileController</servlet-name>
    <servlet-class>controller.Filecontroller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileController</servlet-name>
    <url-pattern>/file</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InsertController</display-name>
    <servlet-name>InsertController</servlet-name>
    <servlet-class>controller.InsertController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertController</servlet-name>
    <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>menu_upload</display-name>
    <servlet-name>menu_upload</servlet-name>
    <servlet-class>controller.menu_upload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>menu_upload</servlet-name>
    <url-pattern>/menuControl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>sub_menu_upload</display-name>
    <servlet-name>sub_menu_upload</servlet-name>
    <servlet-class>controller.sub_menu_upload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sub_menu_upload</servlet-name>
    <url-pattern>/submenuControl</url-pattern>
  </servlet-mapping>
</web-app>